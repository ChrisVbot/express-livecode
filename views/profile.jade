extends layout



block content

  table.u-full-width
    thead
      th= "Name"
      th= "Language"
      th= "Date Created"
      th= "Last Updated"
      th= ""
    tbody
      each val in docs
        tr.document(data-live-code=val.dataValues.id)
          td.documentTitle= val.dataValues.name
          td.documentLanguage= val.dataValues.language
          td.documentCreatedAt= val.dataValues.createdAt
          td.documentUpdatedAt= val.dataValues.updatedAt
          td
              button.viewDoc(data-live-code=val.dataValues.id)= "LiveCode"
        tr.hidden
          td="aaaaaa"


  script(src="/javascripts/profile.js")


