extends layout

block content
  table
    thead
      tr
        td= "Name"
        td= "Language"
        td= "Date Created"
        td= "Last Updated"
    tbody
      each val in docs
        tr.document(data-live-code=val.id)
          td.documentTitle= val.name
          td.documentLanguage= val.language
          td.documentCreatedAt= val.createdAt
          td.documentUpdatedAt= val.updatedAt
          td
            button.viewDoc(data-live-code=val.id)= "LiveCode"
  script(src="/javascripts/profile.js")